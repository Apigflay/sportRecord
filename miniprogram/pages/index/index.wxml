<!-- 云函数 -->
<button 
      open-type="" 
      bindtap='getMessage'
      type="primary"
      class=""
      style=""
      size="mini"
    >点击获取信息</button>
   <button 
      open-type="" 
      bindtap='findMessage'
      type="primary"
      class=""
      style=""
    >点击添加信息</button>
    
<button 
      open-type="" 
      bindtap='setMessage'
      type="primary"
      class=""
      style=""
      size="mini"
    >点击更新信息</button>
         <button 
      open-type="" 
      bindtap='removeMessage'
      type="primary"
      class=""
      style=""
    >点击删除信息</button>



